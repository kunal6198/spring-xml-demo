<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="movie" class="com.stackroute.domain.Movie" autowire="byName">
        <property name="Actor" ref="actor1"></property>
<!--        <constructor-arg name="Actor" ref="actor"/>-->
    </bean>
    <bean id="movie1" class="com.stackroute.domain.Movie">
        <constructor-arg ref="actor2"></constructor-arg>
        <!--        <constructor-arg name="Actor" ref="actor"/>-->
    </bean>

    <bean id="actor1" class="com.stackroute.domain.Actor">
        <property name="name" value="Amitabh"/>
        <property name="age" value="70"/>
        <property name="gender" value="male"/>
    </bean>
    <bean id="actor2" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="Salmaan"/>
        <constructor-arg name="age" value="55"/>
        <constructor-arg name="gender" value="male"/>
    </bean>
    <bean id="actor3" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="Aamir"/>
        <constructor-arg name="age" value="54"/>
        <constructor-arg name="gender" value="male"/>
    </bean>
   <!-- <bean id="movie1" class="com.stackroute.domain.Movie">

        <constructor-arg name="Actor" ref="actor"/>


    </bean>-->

    <bean id="cycle" class="com.stackroute.demo.BeanLifecycleDemoBean" init-method="Init" destroy-method="destroy"></bean>

    <bean id="post" class="com.stackroute.demo.BeanPostProcessorDemoBean"></bean>

</beans>